{
  "outputDirectory": "docs",
  "buildCommand": "python3 scripts/generate-pages.py",
  "installCommand": "",
  "framework": null,
  "cleanUrls": true,
  "trailingSlash": true,
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "X-Content-Type-Options", "value": "nosniff" },
        { "key": "X-Frame-Options", "value": "DENY" }
      ]
    }
  ]
}
